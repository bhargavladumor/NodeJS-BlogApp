<%- include("user_header") %>

    <section class="page-heading wow fadeIn" data-wow-duration="1.5s"
        style="background-image: url(files/images/01-heading.jpg)">
        <div class="container">
            <div class="page-name">
                <h1>Latest Photos</h1>
                <span>Lovely layout of heading</span>
            </div>
        </div>
    </section>

    <section class="portfolio on-portfolio">
        <div class="container">
            <div class="col-sm-12 text-center">
                <div id="projects-filter">
                    <a href="#" data-filter="*" class="active">Show All</a>
                    <% for(let cd of catData) { %>
                        <a href="#" data-filter=".<%= cd.id %>"><%= cd.category_name %></a>
                    <% } %>
                    </div>
            </div>
            <div class="row">
                <div class="row" id="portfolio-grid">

                    <% for(let scd of subcatData){ %>
                    <div class="item col-md-4 col-sm-6 col-xs-12 <%= scd.categoryId %>">
                        <figure>
                            <img alt="1-image" src="<%= scd.catImage %>">
                            <figcaption>
                                <h3><%= scd.title %></h3>
                                <p><%= scd.desc %></p>
                            </figcaption>
                        </figure>
                    </div>
                    <% } %>

                </div>
            </div>
        </div>
    </section>

    <%- include("user_footer") %>